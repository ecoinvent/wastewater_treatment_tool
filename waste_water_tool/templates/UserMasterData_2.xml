<usedUserMasterData xmlns="http://www.EcoInvent.org/UsedUserMasterData">
	{% for entry in ActivityNames %}
	<activityName id="{{ entry.id }}">
    <name xml:lang="en">{{ entry.name }}</name>
  </activityName>
  {% endfor %}
  {% for entry in Sources %}
  <source id="{{ entry.id }}" sourceType="{{ entry.sourceType }}" year="{{ entry.year }}" firstAuthor="{{ entry.firstAuthor }}" 
  {% if entry.additionalAuthors %}additionalAuthors="{{ entry.additionalAuthors }}" {% endif %}
  title="{{ entry.title }}" 
  {% if entry.placeOfPublications %}placeOfPublications="{{ entry.placeOfPublications }}" {% endif %}
  {% if entry.journal %}journal="{{ entry.journal }}" {% endif %}
  {% if entry.namesOfEditors %}namesOfEditors="{{ entry.namesOfEditors }}" {% endif %}
  {% if entry.pageNumbers %}pageNumbers="{{ entry.pageNumbers }}" {% endif %}
  {% if entry.publishers %}publishers="{{ entry.publishers }}" {% endif %}
  {% if entry.titleOfAnthology %}titleOfAnthology="{{ entry.titleOfAnthology }}" {% endif %}
  {% if entry.volumeNo %}volumeNo="{{ entry.volumeNo }}"{% endif %}/>
  {% endfor %}

  {% for entry in Properties %}
  <property id="{{ entry.id }}" 
    {% if entry.defaultVariableName %}defaultVariableName="{{ entry.defaultVariableName}}" {% endif %}
    unitId="{{ entry.unitId }}">
    <name xml:lang="en">{{ entry.name }}</name>
    <unitName xml:lang="en">{{ entry.unitName }}</unitName>
  </property>
  {% endfor %}

  {% for entry in ElementaryExchanges %}
  <elementaryExchange id="{{ entry.id }}" unitId="{{ entry.unitId }}" casNumber="{{ entry.casNumber }}">
    <name xml:lang="en">{{ entry.name }}</name>
    <unitName xml:lang="en">{{ entry.unitName }}</unitName>
    <compartment subcompartmentId="{{ entry.subcompartmentId }}">
      <compartment xml:lang="en">{{ entry.compartment }}</compartment>
      <subcompartment xml:lang="en">{{ entry.subcompartment }}</subcompartment>
    </compartment>
  </elementaryExchange>
  {% endfor %}
  {% for entry in IntermediateExchanges %}
  <intermediateExchange id="{{ entry.id }}" unitId="{{ entry.unitId }}">
    <name xml:lang="en">{{ entry.name }}</name>
    <unitName xml:lang="en">{{ entry.unitName }}</unitName>
    <productInformation />
  </intermediateExchange>
  {% endfor %}
  {% for entry in ActivityIndex %}
	<activityIndexEntry id="{{ entry.id }}" activityNameId="{{ entry.activityNameId }}" geographyId="{{ entry.geographyId }}" startDate="{{ entry.startDate }}" endDate="{{ entry.endDate }}" specialActivityType="{{ entry.specialActivityType }}" systemModelId="{{ entry.systemModelId }}" />
  {% endfor %}

</usedUserMasterData>